<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mad.vastav.keynote">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".memoActivity" />
        <activity android:name=".scribbleActivity" />
        <activity android:name=".checklistActivity" />
        <activity android:name=".quotesActivity" />
        <activity android:name=".EditMemoActivity" />
        <activity android:name=".cameraActivity" />
        <activity android:name=".EditChecklistActivity" />
        <activity android:name=".EditQuotesActivity"/>
       
    </application>

</manifest>